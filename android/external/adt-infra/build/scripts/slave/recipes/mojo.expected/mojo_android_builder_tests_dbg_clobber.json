[
  {
    "cmd": [
      "python",
      "-u",
      "[BUILD]/scripts/slave/bot_update.py",
      "--master",
      "chromium.testing.master",
      "--builder",
      "Mojo Android Builder Tests (dbg)",
      "--slave",
      "TestSlavename",
      "--spec",
      "cache_dir = None\nsolutions = [{'deps_file': 'DEPS', 'managed': True, 'name': 'src', 'url': 'https://chromium.googlesource.com/external/mojo.git'}]\ntarget_os = ['android']",
      "--root",
      "src",
      "--revision_mapping_file",
      "{}",
      "--issue",
      "12853011",
      "--patchset",
      "1",
      "--rietveld_server",
      "https://codereview.chromium.org",
      "--output_json",
      "/path/to/tmp/json",
      "--revision",
      "src@HEAD",
      "--force",
      "--output_manifest"
    ],
    "cwd": "[SLAVE_BUILD]",
    "name": "bot_update",
    "~followup_annotations": [
      "@@@STEP_TEXT@Some step text@@@",
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"did_run\": true, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"manifest\": {}, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"patch_failure\": false, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"patch_root\": \"src\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"properties\": {}, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"root\": \"src\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"step_text\": \"Some step text\"@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "[DEPOT_TOOLS]/gclient.py",
      "runhooks"
    ],
    "cwd": "[SLAVE_BUILD]",
    "name": "gclient runhooks"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[SLAVE_BUILD]/src/mojo/tools/mojob.py",
      "gn",
      "--debug",
      "--android",
      "--dcheck_always_on"
    ],
    "cwd": "[SLAVE_BUILD]/src",
    "env": {
      "GOMA_DIR": "[BUILD]/goma"
    },
    "name": "mojob gn"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[SLAVE_BUILD]/src/mojo/tools/mojob.py",
      "build",
      "--debug",
      "--android",
      "--dcheck_always_on"
    ],
    "cwd": "[SLAVE_BUILD]",
    "env": {
      "GOMA_DIR": "[BUILD]/goma"
    },
    "name": "mojob build"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[SLAVE_BUILD]/src/build/android/provision_devices.py"
    ],
    "cwd": "[SLAVE_BUILD]",
    "name": "provision_device"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[SLAVE_BUILD]/src/build/android/buildbot/bb_device_status_check.py",
      "--json-output",
      "/path/to/tmp/json",
      "--restart-usb"
    ],
    "cwd": "[SLAVE_BUILD]",
    "name": "device_status_check",
    "~followup_annotations": [
      "step returned non-zero exit code: 1",
      "@@@STEP_LOG_LINE@json.output (invalid)@null@@@",
      "@@@STEP_LOG_END@json.output (invalid)@@@",
      "@@@STEP_LINK@report a bug@https://code.google.com/p/chromium/issues/entry?comment=Buildbot%3A+Mojo+Android+Builder+Tests+%28dbg%29%0A%28Please+do+not+change+any+labels%29&labels=Restrict-View-Google%2COS-Android%2CInfra-Client%2CInfra-Labs&summary=Device+Offline+on+chromium.testing.master+TestSlavename@@@",
      "@@@STEP_EXCEPTION@@@"
    ]
  },
  {
    "name": "$result",
    "reason": "Infra Failure: Step('device_status_check') returned 1",
    "status_code": 1
  }
]