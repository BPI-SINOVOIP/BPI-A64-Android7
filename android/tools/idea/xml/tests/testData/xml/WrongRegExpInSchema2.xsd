<schema xmlns="http://www.w3.org/2001/XMLSchema">
  <simpleType name="element-names">
      <list>
        <simpleType>
          <union memberTypes="QName">
            <simpleType>
              <restriction base="string">
                <pattern value="\*"/>
                <pattern value="\i\l\c*:\*"/>
                <pattern value="\\l\\c*:\*"/>
              </restriction>
            </simpleType>
          </union>
        </simpleType>
      </list>
    </simpleType>
</schema>