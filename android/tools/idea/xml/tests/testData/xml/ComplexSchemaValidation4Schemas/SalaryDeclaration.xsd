    <!--

        kernel: Person
    -->
    <xs:complexType name="PersonsType">
        <xs:sequence>
            <xs:element name="Person" type="tns:PersonType" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Person</translation>
