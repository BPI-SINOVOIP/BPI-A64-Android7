<component name="ArtifactManager">
  <artifact name="dir">
    <output-path>$ARTIFACTS_OUT$/dir</output-path>
    <properties id="ant-postprocessing">
      <options enabled="true">
        <file>file://$PROJECT_DIR$/build.xml</file>
        <target>create-file</target>
        <build-properties>
          <build-property name="message.text" value="post" />
        </build-properties>
      </options>
    </properties>
    <properties id="ant-preprocessing">
      <options enabled="true">
        <file>file://$PROJECT_DIR$/build.xml</file>
        <target>show-message</target>
      </options>
    </properties>
    <root id="root">
      <element id="dir-copy" path="$PROJECT_DIR$/data" />
    </root>
  </artifact>
</component>