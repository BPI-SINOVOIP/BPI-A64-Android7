groovy.swing.SwingBuilder.build {
  frame(id: 'f', title: 'Groovy Blogs', visible: true) {
    scrollPane {
      table {
        tableModel(list: items) {
          cols.each { col ->
            closureColumn header: col, read: { it[col].text() }}}}}}
  f.pack()
}
