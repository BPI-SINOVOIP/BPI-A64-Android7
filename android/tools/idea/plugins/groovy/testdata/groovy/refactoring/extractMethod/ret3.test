static def foo(int i, int j, int k) {
    def v
    println(i + j - k)
    v = 42
    <caret>return testMethod(i, v, j)
}

private static int testMethod(int i, int v, int j) {
    if (i > 42) {
        println("preved!")
    } else {
        return v + j
    }
    return 239
}