<merger version="3" >

    <dataSet config="main" >
        <source path="$TOP$$SEP$base" >
            <file
                path="$TOP$$SEP$base$SEP$values$SEP$values.xml"
                qualifiers="" >
                <declare-styleable name="foo" >
        <attr name="bar" format="boolean" />
    </declare-styleable>

                <attr
                    name="bar1"
                    format="boolean" />
            </file>
        </source>
    </dataSet>
    <dataSet config="overlay" >
        <source path="$TOP$$SEP$overlay" >
            <file
                path="$TOP$$SEP$overlay$SEP$values$SEP$values.xml"
                qualifiers="" >
                <attr
                    name="boo"
                    format="boolean" />

                <declare-styleable name="foo" >
        <attr name="bar" />
        <attr name="boo" />
    </declare-styleable>
            </file>
        </source>
    </dataSet>

    <mergedItems>
        <configuration qualifiers="" >
            <declare-styleable name="foo" >
        <attr name="bar" />
        <attr name="boo" />
    </declare-styleable>
        </configuration>
    </mergedItems>

</merger>
