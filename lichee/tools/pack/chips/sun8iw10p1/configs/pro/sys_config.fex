compatible      = "axp157-regulator"
regulator_count = 11
regulator1      = "axp157_dcdc1 none vcc-33 vcc-pd vcc-emmc vcc-usb-30 hpvcc vcc-card vcc-io vcc-ctp audio-33 vcc-emmcv vcc-sdcv vcc-sdcvq33 vcc-sdcvd"
regulator2      = "axp157_dcdc2 none vdd-cpua"
regulator3      = "axp157_dcdc3 none vcc-dram"
regulator4      = "axp157_dcdc4 none vdd-sys"
regulator5      = "axp157_ldo0 none vcc-wifi-33"
regulator6      = "axp157_rtc none vcc-rtc"
regulator7      = "axp157_aldo1 none avcc vcc-pll"
regulator8      = "axp157_aldo2 none vcc-pc vcc-emmcvq18"
regulator9      = "axp157_dldo1 none vcc-wifi-18"
regulator10     = "axp157_dldo2 none"
regulator11     = "axp157_gpioldo none"


;--------------------------------------------------------------------------------------------------------
