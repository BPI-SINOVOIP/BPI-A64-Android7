;----------------------------------------------------------------------------------
; dvfs voltage-frequency table configuration
;
; max_freq: cpu maximum frequency, based on Hz
; min_freq: cpu minimum frequency, based on Hz
;
; LV_count: count of LV_freq/LV_volt, must be < 16
;
; LV1: core vdd is 1.25v if cpu frequency is (912Mhz, 1008Mhz]
; LV2: core vdd is 1.20v if cpu frequency is (768Mhz,  912Mhz]
; LV3: core vdd is 1.10v if cpu frequency is (600Mhz,  768Mhz]
; LV4: core vdd is 1.00v if cpu frequency is (0Mhz,    600Mhz]
; LV5: core vdd is 1.00v if cpu frequency is (0Mhz,    600Mhz]
; LV6: core vdd is 1.00v if cpu frequency is (0Mhz,    600Mhz]
; LV7: core vdd is 1.00v if cpu frequency is (0Mhz,    600Mhz]
; LV8: core vdd is 1.00v if cpu frequency is (0Mhz,    600Mhz]
;
;----------------------------------------------------------------------------------
[dvfs_table]
max_freq = 1008000000
min_freq = 480000000

lv_count = 8

lv1_freq = 1008000000
lv1_volt = 1250

lv2_freq = 912000000
lv2_volt = 1200

lv3_freq = 768000000
lv3_volt = 1100

lv4_freq = 600000000
lv4_volt = 1000

lv5_freq = 0
lv5_volt = 1000

lv6_freq = 0
lv6_volt = 1000

lv7_freq = 0
lv7_volt = 1000

lv8_freq = 0
lv8_volt = 1000

